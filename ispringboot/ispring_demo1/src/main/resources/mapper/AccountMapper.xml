<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ex.mapper.AccountMapper">

    <resultMap id="aRes" type="account">
        <result property="accountName" column="account_name"/>
        <result property="createTime" column="create_time"/>
    </resultMap>
    <select id="queryByAid" resultMap="aRes">
        select id, account_name, password, balance, create_time, uid from account
        where id = #{id}
    </select>

</mapper>
